#include <iostream>
#include <vector>
using namespace std;


void cocktailSort(vector<int> &a, int n){
    int left = 0;
    int right = n-1;
    int k = 0;
    int i;
    while(left < right){
        for(i = left; i < right; i++){
            if(a[i] > a[i+1]){
                swap(a[i], a[i+1]);
                k = i;
            }
            
        }
        right = k;

        for(i = right; i >left; i--){
            if(a[i-1] > a[i]){
                swap(a[i], a[i-1]);
                k = i;
            } 
        }
        left = k;
    }
}
int main(){
    vector<int> a = {23,56,31,0,23,35,0,33};
    int n = a.size();
    cocktailSort(a,n);
    for(int i = 0; i < n; i++) cout<<a[i]<<" ";
    cout<<endl;
    return 0;
}
